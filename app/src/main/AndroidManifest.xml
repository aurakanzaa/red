<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="project.com.simalab">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logosima"
        android:label="@string/app_name"
        android:roundIcon="@drawable/simacircle"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DokterActivity"
            android:label="@string/title_activity_dokter"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".JadwalActivity" />
        <activity android:name=".KtpActivity" />
        <activity android:name=".CariDokterActivity" />
        <activity android:name=".Activity.Booking.FormBookingActivity" />
        <activity android:name=".Activity.Booking.FormBooking2Activity" />
        <activity android:name=".Activity.Booking.BookingActivity"></activity>
    </application>

</manifest>